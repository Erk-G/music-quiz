{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "difficulty",
        "genre",
        "song_title",
        "song_origin",
        "url"
    ],
    "properties": {
        "difficulty": {
            "type": "string",
            "default": "",
            "title": "The difficulty Schema",
            "examples": [
                "easy"
            ]
        },
        "genre": {
            "type": "string",
            "default": "",
            "title": "The genre Schema",
            "examples": [
                "videogame"
            ]
        },
        "song_title": {
            "type": "string",
            "default": "",
            "title": "The song_title Schema",
            "examples": [
                "Sanctuary"
            ]
        },
        "song_origin": {
            "type": "string",
            "default": "",
            "title": "The song_origin Schema",
            "examples": [
                "Kingdom Hearts 2"
            ]
        },
        "url": {
            "type": "string",
            "default": "",
            "title": "The url Schema",
            "examples": [
                "HTUq3Ik1GHM"
            ]
        }
    },
    "examples": [{
        "difficulty": "easy",
        "genre": "videogame",
        "song_title": "Sanctuary",
        "song_origin": "Kingdom Hearts 2",
        "url": "HTUq3Ik1GHM"
    }]
}